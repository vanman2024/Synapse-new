{
  "name": "synapse",
  "version": "1.0.0",
  "description": "AI-powered content automation system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node --transpile-only src/index.ts",
    "dev:skip-types": "nodemon --exec ts-node --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "fix-lint": "eslint --fix 'src/**/*.{js,ts}'",
    "setup": "ts-node scripts/setup.ts",
    "auto-commit": "bash scripts/auto-commit.sh 3",
    "validate": "npm run typecheck && npm run lint",
    "dev:debug": "nodemon --inspect --exec ts-node src/index.ts"
  },
  "keywords": [
    "content",
    "automation",
    "AI",
    "image-generation"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@slack/events-api": "^3.0.1",
    "@slack/web-api": "^6.8.1",
    "airtable": "^0.12.1",
    "axios": "^1.3.4",
    "bull": "^4.10.4",
    "cheerio": "^1.0.0-rc.12",
    "cloudinary": "^1.35.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.8.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^3.2.1",
    "sharp": "^0.32.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}